- content_for(:title, "新規登録")
.container
  .center-block.col-md-6.mx-auto
    %h2.text-success.border-bottom.text-center 新規登録
    = form_with model: @user, url: user_registration_path, local: true do |f|
      = render partial: "devise/registrations/registrations_form", locals: {obj: f, user: @user, is_new_user_registration_path: true}
